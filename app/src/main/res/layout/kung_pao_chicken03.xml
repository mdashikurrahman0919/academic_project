<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#01dfd7"
    tools:ignore="ExtraText">

    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="398dp"
        android:layout_height="276dp"
        android:onClick="fourth_food_list"
        android:scaleType="fitXY"
        android:src="@drawable/kung4"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/kung4"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView53"
        android:layout_width="392dp"
        android:layout_height="115dp"
        android:background="@color/white"
        android:text="@string/kung5"
        android:textColor="@color/green"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView30"
        tools:ignore="TextSizeCheck" />

    <ImageView
        android:id="@+id/imageView32"
        android:layout_width="403dp"
        android:layout_height="138dp"
        android:scaleType="fitXY"
        android:src="@drawable/kung5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView53"
        app:srcCompat="@drawable/kung5" />

    <Button
        android:id="@+id/button5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:onClick="fourth_food_list"
        android:text="@string/kung10"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView32" />



</androidx.constraintlayout.widget.ConstraintLayout>